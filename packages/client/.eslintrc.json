{
  "root": true,
  "extends": "airbnb",
  "plugins": ["import"],
  "env": {
    "browser": true
  },
  "rules": {
    "react/jsx-filename-extension": [
      "error",
      { "extensions": [".js", ".jsx"] }
    ],
    "react/prefer-stateless-function": "off"
  },
  "settings": {
    "import/resolver": {
      "babel-module": {}
    }
  }
}
